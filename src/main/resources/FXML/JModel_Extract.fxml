<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.battledash.jmodel.Methods.Controllers.JModelExport">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <bottom>
            <HBox prefHeight="46.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <children>
                  <Region prefHeight="25.0" prefWidth="505.0" />
                  <Button fx:id="ExportButton" mnemonicParsing="false" onMouseClicked="#onExportButtonClicked" text="Export" />
               </children>
            </HBox>
         </bottom>
         <center>
            <BorderPane prefHeight="234.0" prefWidth="455.0" BorderPane.alignment="CENTER">
               <center>
                  <VBox alignment="CENTER" prefHeight="365.0" prefWidth="600.0" spacing="5.0" BorderPane.alignment="CENTER">
                     <children>
                        <CheckBox fx:id="JSONExportCheckBox" mnemonicParsing="false" text="JSON" />
                        <CheckBox fx:id="SoundExportCheckBox" mnemonicParsing="false" text="Sound" />
                        <CheckBox fx:id="ImageExportCheckBox" mnemonicParsing="false" text="Image" />
                        <CheckBox fx:id="UAssetExportCheckBox" mnemonicParsing="false" text="UAsset" />
                        <CheckBox fx:id="UBulkExportCheckBox" mnemonicParsing="false" text="UBulk" />
                        <CheckBox fx:id="UExpExportCheckBox" mnemonicParsing="false" text="UExp" />
                     </children>
                     <BorderPane.margin>
                        <Insets bottom="5.0" />
                     </BorderPane.margin>
                  </VBox>
               </center>
            </BorderPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
